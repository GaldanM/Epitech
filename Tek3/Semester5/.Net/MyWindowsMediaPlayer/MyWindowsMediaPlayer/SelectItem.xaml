<Window x:Class="MyWindowsMediaPlayer.SelectItem"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyWindowsMediaPlayer"
        mc:Ignorable="d"
        Icon="taskbar.ico"
        Title="Choice" Height="300" Width="300" ResizeMode="NoResize">
    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="230" Margin="0,10,0,0" VerticalAlignment="Top" Width="292"/>
        <Button x:Name="OkButton" Content="Ok" HorizontalAlignment="Left" Margin="0,240,0,0" VerticalAlignment="Top" Width="292" Height="30" Click="button_Click"/>

    </Grid>
</Window>
